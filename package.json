{
  "name": "node-express-pug-react",
  "version": "0.0.1",
  "description": "Phoenix ReactJS Meetup Lightning Talk Demo – May 2018",
  "main": "server/index.js",
  "repository": "https://github.com/elm-street-technology/node-express-pug-react",
  "scripts": {
    "start": "concurrently 'yarn start:server' 'yarn start:webpack'",
    "start:server": "nf run -- nodemon server/index.js --exec babel-node",
    "start:webpack": "nf run -- webpack --watch --mode=development --config webpack.config.js"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "elevate-ui": "0.1.0",
    "express": "4.16.3",
    "formik": "0.11.11",
    "pug": "2.0.3",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "yup": "0.24.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "concurrently": "3.5.1",
    "foreman": "^2.0.0",
    "glob": "7.1.2",
    "nodemon": "1.17.4",
    "prettier": "1.12.1",
    "webpack": "4.7.0",
    "webpack-cli": "2.1.3"
  },
  "author": "Chris Heninger",
